msgid "rotate by -90 degrees"
msgstr "Поворот на -90°"

#: ../src/iop/flip.c:426
msgid "rotate by  90 degrees"
msgstr "Поворот на 90°"

#: ../src/iop/flip.c:430
msgid "rotate by 180 degrees"
msgstr "Поворот на 180°"

#: ../src/iop/flip.c:505
msgid "rotate"
msgstr "Вращение"

